{
  "name": "pakag",
  "description": "Format package.json contents when editing.",
  "version": "0.0.0-development",
  "repository": "github:tobua/pakag",
  "scripts": {
    "test": "vitest run"
  },
  "padua": {
    "source": true
  },
  "dependencies": {
    "prettier": "^2.8.8",
    "sort-package-json": "^2.4.1"
  },
  "type": "module",
  "sideEffects": true,
  "main": "index.js",
  "exports": {
    ".": {
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "source": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "devDependencies": {
    "padua": "^1.3.0",
    "vitest": "^0.32.0"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "import/extensions": [
        2,
        "ignorePackages"
      ]
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "publishConfig": {
    "provenance": true
  }
}
