{
  "name": "pakag",
  "description": "Format package.json contents when editing.",
  "version": "2.1.0",
  "repository": "github:tobua/pakag",
  "scripts": {
    "test": "padua test"
  },
  "padua": {
    "source": true
  },
  "dependencies": {
    "prettier": "^2.1.2",
    "sort-package-json": "^1.46.0"
  },
  "type": "module",
  "main": "index.js",
  "source": "index.js",
  "types": "index.d.ts",
  "files": [
    "**/*.js"
  ],
  "devDependencies": {
    "padua": "^0.1.1"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "import/extensions": [
        2,
        "always"
      ]
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./node_modules/padua/configuration/.babelrc"
        }
      ]
    }
  },
  "engines": {
    "node": ">= 13.2.0"
  }
}
